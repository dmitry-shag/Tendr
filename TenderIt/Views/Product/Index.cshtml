@model IEnumerable<TenderIt.Models.ProductTable>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Товары</h2>

<table border="1">
    <tr>
        <td><p style="margin-left: 5px;">Название</p></td>
    </tr>
    @foreach (TenderIt.Models.ProductTable item in Model)
    {
    <tr>
        <td><p style="margin-left: 5px;">@item.Name</p></td>
    </tr>
    }
</table>
